class Solution:
    def halvesAreAlike(self, s: str) -> bool:
        c1=0
        c2=0
        v=['a','e','i','o','u','A','E','I','O','U']
        for i in range(0,int(len(s)/2)):
            if s[i] in v:
                c1+=1
        for i in range(int(len(s)/2),len(s)):
            if s[i] in v:
                c2+=1
        if c1==c2:
            return True
        else:
            return False
        
